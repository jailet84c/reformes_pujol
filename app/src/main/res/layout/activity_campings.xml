<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Campings">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        app:theme = "@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#3F51B5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ivGualba"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@drawable/logogualba" />

    <ImageView
        android:id="@+id/logorocagrossa"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="50dp"
        android:background="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ivGualba"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@drawable/rocagrossalogo" />

    <ImageView
        android:id="@+id/logobergaresort"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivGualba"
        app:srcCompat="@drawable/logobergaresort" />

    <ImageView
        android:id="@+id/logokanguro"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/logobergaresort"
        app:layout_constraintTop_toBottomOf="@+id/logorocagrossa"
        app:srcCompat="@drawable/logokanguro" />

    <ImageView
        android:id="@+id/logomasjosep"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logobergaresort"
        app:srcCompat="@drawable/logomassantjosep" />

    <ImageView
        android:id="@+id/logotossa"
        android:layout_width="100sp"
        android:layout_height="100sp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/logomasjosep"
        app:layout_constraintTop_toBottomOf="@+id/logokanguro"
        app:srcCompat="@drawable/mapatossa" />
</androidx.constraintlayout.widget.ConstraintLayout>