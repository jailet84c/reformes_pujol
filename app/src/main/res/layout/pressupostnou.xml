        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/nouetData"
        app:layout_constraintTop_toTopOf="@+id/rectdadesclient" />

    <EditText
        android:id="@+id/nouetNomP"
        android:layout_width="0dp"
        android:layout_height="40sp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:autofillHints=""
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textMultiLine|textPersonName"
        android:maxLength="20"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/rectdadesclient"
        app:layout_constraintStart_toEndOf="@+id/noutvNomC"
        app:layout_constraintTop_toTopOf="@+id/rectdadesclient" />

    <TextView
        android:id="@+id/noutvCampingP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:text="@string/campingPres"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/rectdadesclient"
        app:layout_constraintStart_toStartOf="@+id/rectdadesclient"
        app:layout_constraintTop_toBottomOf="@+id/noutvData" />

    <EditText
        android:id="@+id/nouetCampingP"
        android:layout_width="120sp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:autofillHints=""
        android:background="@android:color/transparent"
        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ abcdefghijklmnopqrstuvwxyz0123456789"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectdadesclient"
        app:layout_constraintStart_toEndOf="@+id/noutvCampingP"
        app:layout_constraintTop_toBottomOf="@+id/nouetData" />

    <ImageView
        android:id="@+id/rectconcepte"
        android:layout_width="0dp"
        android:layout_height="35sp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:background="#DDFFFFFF"
        android:contentDescription="@string/todo6"
        android:src="@drawable/rectanglenegre"
        app:layout_constraintEnd_toEndOf="parent"
