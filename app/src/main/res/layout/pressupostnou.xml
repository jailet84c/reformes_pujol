    <TextView
        android:id="@+id/noutvNRai"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="@string/_695010028"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/noutvRai"
        app:layout_constraintTop_toBottomOf="@+id/nounomEmpresa" />

    <TextView
        android:id="@+id/noutvNEdu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/_605847675"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/noutvEdu"
        app:layout_constraintTop_toBottomOf="@+id/noutvNRai" />

    <ImageView
        android:id="@+id/rectdadesclient"
        android:layout_width="0dp"
        android:layout_height="100sp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:contentDescription="@string/todo5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rectimage"
        app:srcCompat="@drawable/rectanglenegre" />

    <TextView
        android:id="@+id/noutvData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
