        app:layout_constraintTop_toBottomOf="@+id/nouetnumero1" />

    <ImageView
        android:id="@+id/noulogoempresa"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectimage"
        app:layout_constraintEnd_toEndOf="@+id/rectimage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/noutvNRai"
        app:layout_constraintTop_toTopOf="@+id/rectimage"
        app:srcCompat="@drawable/reformespujol" />

    <TextView
