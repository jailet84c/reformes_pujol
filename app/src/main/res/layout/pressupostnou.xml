        android:text="€"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/rectconcepte3"
        app:layout_constraintEnd_toEndOf="@+id/rectconcepte3"
        app:layout_constraintStart_toEndOf="@+id/nouetTotal"
        app:layout_constraintTop_toTopOf="@+id/rectconcepte3" />

    <EditText
        android:id="@+id/nouetTotalIva"
        android:layout_width="60sp"
        android:layout_height="20sp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="2dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:gravity="end"
        android:inputType="phone"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/rectconcepte3"
