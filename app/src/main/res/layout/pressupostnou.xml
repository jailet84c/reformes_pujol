        android:background="@android:color/transparent"
        android:ems="10"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/noutvData"
        app:layout_constraintTop_toTopOf="@+id/rectdadesclient" />

    <TextView
        android:id="@+id/noutvNomC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
